{
  "functions": {
    "pages/api/update/*": {
      "memory": 3008,
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/update/updateAllPrices",
      "schedule": "*/10 * * * *"
    },
    {
      "path": "/api/update/updateAllTokenSlots",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/update/updateMinaPin",
      "schedule": "0 */2 * * *"
    },
    {
      "path": "/api/update/updateMinaTxn",
      "schedule": "1 */2 * * *"
    },
    {
      "path": "/api/update/updateHistorical",
      "schedule": "*/30 * * * *"
    }
  ]
}
