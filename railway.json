{
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "npm start",
    "cronJobs": [
      {
        "name": "updateAllPrices",
        "command": "curl -X POST http://localhost:3000/api/update/core/updateAllPrices",
        "schedule": "*/10 * * * *"
      },
      {
        "name": "updateDootZeko",
        "command": "curl -X POST http://localhost:3000/api/update/core/updateDootZeko",
        "schedule": "1-59/20 * * * *"
      },
      {
        "name": "updateDootMina",
        "command": "curl -X POST http://localhost:3000/api/update/core/updateDootMina",
        "schedule": "11-59/20 * * * *"
      },
      {
        "name": "updateHistorical",
        "command": "curl -X POST http://localhost:3000/api/update/ipfs/updateHistorical",
        "schedule": "5-59/10 * * * *"
      }
    ]
  }
}
